{
    "npm.packageManager": "yarn",
    "css.validate": false,
    "less.validate": false,
    "scss.validate": false,
    "path-autocomplete.pathMappings": {
        "~": "${folder}",
        "@": "${folder}/src/"
    },
    "[scss]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode",
        "editor.codeActionsOnSave": {
            "source.fixAll.stylelint": "explicit"
        }
    },
    "[vue]": {
        "editor.codeActionsOnSave": {
            "source.fixAll": "explicit"
        }
    },
    "stylelint.snippet": ["css", "sass", "scss", "postcss", "vue"],
    "stylelint.validate": ["css", "sass", "scss", "postcss", "vue"],
    "javascript.suggest.paths": true,
    "typescript.suggest.paths": true,
    "cSpell.words": [
        "AISEO",
        "beian",
        "categoryid",
        "CCONFIG",
        "clienttype",
        "cnzz",
        "colspan",
        "colwidth",
        "composables",
        "copyid",
        "dotlottie",
        "expiredtime",
        "filemanager",
        "fontid",
        "ibgmeta",
        "iconpark",
        "imagelink",
        "immer",
        "industryids",
        "lable",
        "linkifyjs",
        "linktarget",
        "linkurl",
        "listid",
        "lottiefiles",
        "lottietype",
        "mediaurl",
        "mourl",
        "msnry",
        "msuid",
        "mvars",
        "noreferrer",
        "npid",
        "nprogress",
        "oldlistid",
        "pexels",
        "photoswiper",
        "postcid",
        "postcontent",
        "postdescription",
        "postidp",
        "postimage",
        "postkeywords",
        "postlang",
        "postldp",
        "postlinktarget",
        "postlinkurl",
        "postmedia",
        "postmeta",
        "postmlimit",
        "postndp",
        "postoldurl",
        "postpid",
        "postremarks",
        "postsubtitle",
        "posttags",
        "posttheme",
        "posttitle",
        "posttype",
        "posturl",
        "postusetdate",
        "prismjs",
        "Prosemirror",
        "pswp",
        "qiniu",
        "rowspan",
        "selectednode",
        "seotitle",
        "Seting",
        "singlescreen",
        "sitetplurl",
        "siteurl",
        "spage",
        "splinecode",
        "splinetool",
        "Splite",
        "svgfont",
        "svgicons",
        "svgo",
        "svgtofont",
        "talign",
        "Tiptap",
        "toastification",
        "uebuilder",
        "ueditor",
        "UEENGINE",
        "uetplicon",
        "Ukiyo",
        "ukiyojs",
        "unloadable",
        "unplugin",
        "valign",
        "videom",
        "Zoomable"
    ],
    "typescript.tsdk": "node_modules/typescript/lib",
    "svg.preview.background": "transparent",
    "vue.codeActions.enabled": false,
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/node_modules/": true
        // "**/uebuilder-uemo/": true,
        // "**/uebuilder-tools/": true,
        // "**/uebuilder-custom/": true,
        // "**/uebuilder-mo005/": true,
        // "**/uebuilder-base/": true,
        // "**/uebuilder-test/": true,
        // "**/uebuilder-editor/app-preview/": true,
        // "**/uebuilder-editor/app-editor/*.*": true,
    },
    "editor.wordWrapColumn": 120,

    "i18n-ally.namespace": true,
    "i18n-ally.localesPaths": ["uebuilder-plugins/uemo-editor-element/i18n"],
    "i18n-ally.keystyle": "nested",
    "i18n-ally.displayLanguage": "zh-cn",
    "i18n-ally.sourceLanguage": "zh-cn",
    "i18n-ally.enabledFrameworks": ["vue"],
    "i18n-ally.theme.annotationMissing": "#acb2be",

    "vue.server.hybridMode": false

    // "http.proxy": "http://127.0.0.1:10809",
    // "http.proxyStrictSSL": false
}
